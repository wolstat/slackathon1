{
  "name": "TSM Slack helper",
  "short_name": "SlackHelp",
  "description": "This extension will alert you to new messages in Slack",
  "version": "0.13",
  "manifest_version": 2,
  "homepage_url": "https://github.com/wolstat/slackathon1",
  "author":"Michael Wolstat, Jonathan Zee, Adrian Borromeo, Jack Nealy",
  "update_url": "https://github.com/wolstat/slackathon1/raw/master/update-manifest.xml",
  "oauth2": {
    "client_id": "908477540370-rr8mk5rd5etaeq13v8bmn7fh1u4s2a68.apps.googleusercontent.com",
    "scopes": [
      "https://slack.com/oauth/authorize"
    ]
  },
  "icons": {
    "128": "img/icon_128.png"
  },
  "background": {
    "scripts": ["js/jquery-2.1.3.min.js", "js/jquery.easing.1.3.js", "js/background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {   
      "19": "img/icon19.png", 
      "38": "img/icon38.png",
      "128": "img/icon_128.png"
    },
    "default_popup": "index.html",
    "default_title": "Slack Ext"
  },
  "commands": {
    "open-google": {
      "description": "Open a tab to google.com",
      "suggested_key": { "default": "Ctrl+Shift+L" }
    },
    "_execute_browser_action": {
      "suggested_key": { "default": "Ctrl+Shift+K" }
    }
  },
  "permissions": [
    "alarms", 
    "tabs",
    "idle",
    "identity",
    "storage",
    "activeTab",
    "http://*.slack.com/*",
    "https://*.slack.com/*",
    "http://slack.com/*",
    "https://slack.com/*"
  ]
}